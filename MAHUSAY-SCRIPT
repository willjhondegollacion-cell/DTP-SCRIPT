local Key, AuthF, HopF, StatsF = "2011", "Auth.json", "Hops.json", "Stats.json"
local HS, TS, LP = game:GetService("HttpService"), game:GetService("TeleportService"), game.Players.LocalPlayer
local RS = game:GetService("RunService")

local function save(f, d) pcall(function() writefile(f, HS:JSONEncode(d)) end) end
local function load(f) 
    local s, r = pcall(function() return HS:JSONDecode(readfile(f)) end)
    return s and r or {} 
end

local auth, visited, stats = load(AuthF), load(HopF), load(StatsF)
stats.TotalClaims = stats.TotalClaims or 0

-- UI Neon Design
local sg = Instance.new("ScreenGui", game.CoreGui)
local mainFrame = Instance.new("Frame", sg)
mainFrame.Size, mainFrame.Position = UDim2.new(0, 350, 0, 60), UDim2.new(0.5, -175, 0, 30)
mainFrame.BackgroundColor3, mainFrame.BackgroundTransparency = Color3.fromRGB(5, 5, 5), 0.1
Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0, 10)
local uiStroke = Instance.new("UIStroke", mainFrame)
uiStroke.Thickness, uiStroke.Color = 3, Color3.fromRGB(0, 255, 255)

local statusLabel = Instance.new("TextLabel", mainFrame)
statusLabel.Size, statusLabel.BackgroundTransparency = UDim2.new(1, 0, 1, 0), 1
statusLabel.TextColor3, statusLabel.Font, statusLabel.TextSize = Color3.new(1, 1, 1), Enum.Font.GothamBold, 18
statusLabel.Text = "TURBO ACTIVATED..."

task.spawn(function() while task.wait() do uiStroke.Color = Color3.fromHSV(tick() % 3 / 3, 1, 1) end end)

local function start()
    if not game:IsLoaded() then game.Loaded:Wait() end
    local char = LP.Character or LP.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    
    -- Nameless Stealth
    char:WaitForChild("Humanoid").DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
    for _, p in pairs(char:GetDescendants()) do 
        if p:IsA("BasePart") then p.Transparency = 1 
        elseif p:IsA("Decal") or p:IsA("Accessory") then p:Destroy() end 
    end

    workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
    RS.Heartbeat:Connect(function()
        if hrp then workspace.CurrentCamera.CFrame = CFrame.new(hrp.Position + Vector3.new(0,12,0), hrp.Position) end
    end)
    
    visited[game.JobId] = true; save(HopF, visited)
    local unclaim = nil
    for _, r in pairs(game:GetDescendants()) do 
        if r:IsA("RemoteEvent") and (r.Name:lower():find("unclaim") or r.Name:lower():find("leave")) then unclaim = r break end 
    end

    local function doHop()
        statusLabel.Text = "FAST HOPPING..."
        save(StatsF, stats)
        local success, result = pcall(function()
            return HS:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Desc&limit=100")).data
        end)
        
        if success and result then
            for _, v in pairs(result) do 
                if v.id ~= game.JobId and not visited[v.id] and v.playing < v.maxPlayers then 
                    TS:TeleportToPlaceInstance(game.PlaceId, v.id) 
                    return 
                end 
            end
        end
        visited = {}; save(HopF, visited); TS:Teleport(game.PlaceId) -- Unlimited
    end

    task.spawn(function()
        local bl, lastClaim, session = {}, tick(), 0
        while true do
            local found = false
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("ProximityPrompt") and v.ActionText:find("Claim") and not bl[v.Parent] then
                    found, bl[v.Parent] = true, true
                    
                    -- AGGRESSIVE SPEED: Reduced Delays
                    hrp.CFrame = v.Parent.CFrame
                    task.wait(0.04) 
                    session = session + 1
                    statusLabel.Text = "TURBO CLAIM: " .. session
                    fireproximityprompt(v)
                    
                    if unclaim then task.spawn(function() unclaim:FireServer() end) end
                    task.wait(0.04)
                    lastClaim = tick()
                end
            end
            -- Faster Anti-Stuck
            if not found or (tick() - lastClaim > 4) then doHop() break end 
            task.wait(0.2)
        end
    end)
end

-- PASSWORD GATE (2011)
if auth.ID == LP.UserId then start() else
    local kg = Instance.new("ScreenGui", game.CoreGui); local kf = Instance.new("Frame", kg)
    kf.Size, kf.Position, kf.BackgroundColor3 = UDim2.new(0,250,0,120), UDim2.new(0.5,-125,0.5,-60), Color3.fromRGB(20,20,20)
    Instance.new("UICorner", kf).CornerRadius = UDim.new(0,8)
    local ti = Instance.new("TextBox", kf); ti.Size, ti.Position, ti.PlaceholderText = UDim2.new(0.8,0,0,35), UDim2.new(0.1,0,0.2,0), "Password?"
    ti.Text = ""; ti.BackgroundColor3 = Color3.fromRGB(40,40,40); ti.TextColor3 = Color3.new(1,1,1)
    local tb = Instance.new("TextButton", kf); tb.Size, tb.Position, tb.Text = UDim2.new(0.8,0,0,35), UDim2.new(0.1,0,0.6,0), "LOGIN"
    tb.BackgroundColor3 = Color3.fromRGB(0, 150, 255); tb.TextColor3 = Color3.new(1,1,1)
    tb.MouseButton1Click:Connect(function()
        if ti.Text == Key then auth.ID = LP.UserId; save(AuthF, auth); kg:Destroy(); start() else LP:Kick("SAYOP!") end
    end)
end
